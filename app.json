{
    "expo": {
        "plugins": [
            [
                "expo-notifications",
                {
                    "icon": "./local/assets/notification-icon.png",
                    "color": "#ffffff",
                    "sounds": ["./local/assets/notification-sound.wav", "./local/assets/notification-sound-other.wav"],
                    "mode": "production"
                }
            ]
        ],
        "name": "SpottyFind",
        "slug": "spottyfind",
        "version": "1.0.5",
        "orientation": "portrait",
        "icon": "./img/logo.png",
        "splash": {
            "image": "./img/splash.png",
            "backgroundColor": "#2CCB33",
            "resizeMode": "contain"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "permissions": ["NOTIFICATIONS"],
            "versionCode": 6,
            "adaptiveIcon": {
                "foregroundImage": "./img/logo.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.zeekoe.spottyfind",
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyCYQa881E4BoFyJ50iqcPSqDv2OmKSTjkI"
                }
            },
            "googleServicesFile": "./google-services.json"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "description": ""
    }
}
